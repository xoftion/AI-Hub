# AI Provider API Keys
OPENAI_API_KEY=your_openai_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
DEEPSEEK_API_KEY=your_deepseek_api_key_here
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Alternative environment variable names (fallbacks)
OPENAI_API_KEY_ENV_VAR=your_openai_api_key_here
GEMINI_API_KEY_ENV_VAR=your_gemini_api_key_here
DEEPSEEK_API_KEY_ENV_VAR=your_deepseek_api_key_here
ELEVENLABS_API_KEY_ENV_VAR=your_elevenlabs_api_key_here

# Database URL (if using database instead of memory storage)
DATABASE_URL=postgresql://username:password@localhost:5432/ai_api_hub

# Frontend Configuration (for CORS)
FRONTEND_URL=http://localhost:5000
CORS_ORIGIN=http://localhost:5000

# Server Configuration
NODE_ENV=development
PORT=5000

# Production URLs (replace with your actual deployment URLs)
# FRONTEND_URL=https://your-app.vercel.app
# CORS_ORIGIN=https://your-app.vercel.app
